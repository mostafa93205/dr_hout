# Use npm registry
registry=https://registry.npmjs.org/

# Disable strict SSL for troubleshooting
strict-ssl=false

# Use legacy peer deps to avoid dependency conflicts
legacy-peer-deps=true

# Disable use-node-version to avoid version conflicts
use-node-version=false

# Set network timeout higher
network-timeout=100000

# Disable fetch retries to avoid ERR_INVALID_THIS errors
fetch-retries=0
